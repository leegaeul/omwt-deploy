    function chkMaxLenDisp(obj, targetId, maxLen) {
        $(obj).attr('maxlength', maxLen);
        var len = $(obj).val().length;

        if ( len > maxLen ) {
            try {
                len = maxLen;

                $('body').focus();
                obj.value = obj.value.substr(0, maxLen);
                $("#" + obj.id).focus();
                $("#" + obj.id).selectRange(len, len);
            } catch(e) {
            }
            alert("작품의 줄거리는 1000글자 이내로 입력 가능합니다");
            return false;
        }
        $('#'+targetId).html(len);
    };
    
