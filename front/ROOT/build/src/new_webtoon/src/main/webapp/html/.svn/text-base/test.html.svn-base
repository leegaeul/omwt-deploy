	var face_login_check = "fail";
	var FacebookAccessToken = "";
	function fbSignIn() {	
		/* console.log("fbSignIn"); */
		FB.getLoginStatus(function(response) {
			if (response.status == 'connected') {
				if (response.authResponse) {
					FacebookAccessToken = response.authResponse.accessToken;
					face_login_check = "success";
					fbWrite();
				} else {
					alert('Facebook 인증이 되지 않았습니다.');
					return;
				}
			} else {  //로그인이 안되 있을 경우 로그인 창과 퍼미션 창을 띄움
				FB.login(function(response) {
					if (response.status  == 'connected') {
						FacebookAccessToken = response.authResponse.accessToken;
						face_login_check = "success";
						
						fbWrite();
					} else if (response.status == 'not_authorized') {
						alert("권한요청에 동의해 주셔야 정상적인 이벤트 참여가 가능합니다.");
						return;
					}
				}, {scope: 'public_profile, publish_actions', return_scopes: true, auth_type: 'rerequest'});
			}
		});
	}
	
	function fbWrite() {

		
		if(face_login_check == "fail") {
			fbSignIn();
			return;
		}
		
		var result = "success";
		
		var publish = {
		  method: 'feed',
		  message: mContent,
		  name: '제2회 올레마켓웹툰 공모전 소문내기 비정상 이벤트',
		  caption: '올레마켓웹툰 소문내기 이벤트',
		  description: (
		      '꿈을 현실로~Dream Comes True 올레마켓웹툰 공모전 열심히 소문내고 비타민 워터 받자!'
		  ),
		  link: 'http://webtoon.olleh.com/eventpromotion/contestEvent.kt',
		  picture: 'https://webtoon.olleh.com/images/pc/event/contest/ic_contest.png',
		  actions: [
		    { name: '제2회 올레마켓웹툰 공모전 소문내기 비정상 이벤트', link: 'http://webtoon.olleh.com/eventpromotion/contestEvent.kt' }
		  ],
		  user_message_prompt: mContent
		};
	
		FB.ui(publish, function(response) {
		    if (response && !response.error_code) {
		    	fncRegistEventProc('facebook');
		    } else {
		    	result = "fail";
		    }
		  });
	
		return result;
		
	}
	
