	//팝업창 초기화 
	function init_popup(popup)
	{	
	
		$('.'+popup).fadeIn('fast');
		var popupY = 0;
		if ($(window).height() < $('.'+popup).outerHeight())
		{
			popupY = $(window).scrollTop();
		}
		else
		{
			popupY = $(window).scrollTop() + ($(window).height()/2) - ($('.'+popup).outerHeight()/2);
		}
		popupY = popupY + 70;
		$('.'+popup).css({ top: popupY });
		$('.com_popup_bglayer').css({opacity: 0, height: $(document).height()});
		//$('.com_popup_bglayer').show();
	
	}
	
	/* 디자인 체크박스 */
    function designCheckboxinit(){
        var $radioWrapper = $('.category_list'), //라디오 버튼 전체 영역
            $radioIist = $('.category_list > li'), //라디오 버튼 리스트
            $checkInputFake = $('.category_list > li > .design_button'), //라디오 버튼 영역
            $checkInput = $('.category_list > li > input'), //라디오 버튼 영역
            $radioLabel = $('.category_list > li > label'), //레이블 버튼 영역
            $startIdx = 0, //라디오 버튼 초기화
            onIndex = -1 //현재활성화 라디오 버튼 초기화

        function init(){
            $checkInput.each(function (idx, entry){
                $(entry).data('input', idx);
            });
            $radioIist.each(function (idx, entry){
                $(entry).data('label', idx);
            });
            startIdx($startIdx); //탭 초기 활성화
            
        };
        init(); //초기화

        //처음 이벤트 실행될 함수
        function startIdx(idx){
        	 for (var i = 0; i < 6; i++) {
        		 
        		 if(i==0){
        			 $radioIist.eq(i).addClass('select').children("input").attr("checked", "checked");
        		 }else{
        			 $radioIist.eq(i).removeClass('select').children("span").css("outline", "none");
        			 $radioIist.eq(i).children("input").attr("checked", false);
        			 
        		 }
        		
                 $checkInput.css({position:'absolute', left:-9999});
             
             }
             
        };

 
    };
    
