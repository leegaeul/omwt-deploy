//파일 업로드 validation
function beforeUpload( formData, jqForm, options )
{
	
	if(jqForm.find("input[type=file]").val()=="" || jqForm.find("input[type=file]").val() == null){
		alert("업로드할 파일을 선택해 주세요.");
		return false;
	}

	return true; 
}

//파일 업로드 결과 처리
function onUploadSuccess( uploadData )
{
	
	var filechecktmp = uploadData.fileName;
	var filecheckarr = filechecktmp.split('.');
	var filecheck = filecheckarr[1];
	var filechecksize = uploadData.fileSize;
	
	if(filechecksize > 314572800){
		alert("파일 용량은 300MB까지 입니다.\n파일 사이즈를 좀 더 최적화해주시거나,\n압축 파일로 올려주세요.");
		return false;
	}

	if(filecheck != "zip"){
		alert("파일 형식을\n다시 한번 확인 해주세요.");
		return false;
	} 
	
	jQuery('#fileoname').val(uploadData.originFileName);
	jQuery('#filename').val(uploadData.fileName);
	jQuery('#filepath').val(uploadData.filePath);
	
	alert('파일 업로드가 완료되었습니다.');
	return true;
	
}

