	//아이디 구분(open, olleh)후 **처리
	function fncChangeId(domain){

		if(domain.idfg == "olleh")
		{
			if(domain.regid.indexOf('@') > -1)
			{
				return domain.regid.indexOf('@') > 3 ? domain.regid.substring(0, 3) + addChar(domain.regid.indexOf('@') - 3, '*') + domain.regid.substring(domain.regid.indexOf('@')) : domain.regid;
			}
			
			return domain.regid.length > 3 ? domain.regid.substring(0, 3) + addChar(domain.regid.length - 3, '*') : domain.regid;
		}
		
		return domain.nickname;
	}

	function addChar(cnt, ch){
		
		return cnt == 1 ? ch : ch + addChar(cnt - 1, ch); 
	}
     
