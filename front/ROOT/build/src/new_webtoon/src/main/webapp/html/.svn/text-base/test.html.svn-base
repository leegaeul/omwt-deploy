	function fncSetCommentResult(json){
		
		var html = "";
		for(var i = 0; i < json.snsCommentList.length; i++)
		{	
			html += "<li>";
			html += "    <dl>";
			
			if(json.snsCommentList[i].snsfg == "facebook"){
				html += "        <dt><img src=\"/images/pc/event/contest/completition_icon_facebook.jpg\" alt=\"페이스북 사용자\" /></dt>";
			} else if(json.snsCommentList[i].snsfg == "twitter"){
