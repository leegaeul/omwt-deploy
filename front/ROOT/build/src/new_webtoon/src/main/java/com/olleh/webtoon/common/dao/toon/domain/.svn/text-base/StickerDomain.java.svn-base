package com.olleh.webtoon.common.dao.toon.domain;

public class StickerDomain 
{
	private int    timesseq   = 0;
	private int    rankingseq = 0;
	private String idfg       = "";
	private String regid      = "";
	private int    stickercnt = 0;
	private String regdt      = "";
	
	public int getTimesseq() { return timesseq; }
	public void setTimesseq(int timesseq) { this.timesseq = timesseq; }
	
	public int getRankingseq() { return rankingseq; }
	public void setRankingseq(int rankingseq) { this.rankingseq = rankingseq; }
	
	public String getIdfg() { return idfg; }
	public void setIdfg(String idfg) { this.idfg = idfg; }
	
	public String getRegid() { return regid; }
	public void setRegid(String regid) { this.regid = regid; }
	
	public int getStickercnt() { return stickercnt; }
	public void setStickercnt(int stickercnt) { this.stickercnt = stickercnt; }
	
	public String getRegdt() { return regdt; }
	public void setRegdt(String regdt) { this.regdt = regdt; }
}