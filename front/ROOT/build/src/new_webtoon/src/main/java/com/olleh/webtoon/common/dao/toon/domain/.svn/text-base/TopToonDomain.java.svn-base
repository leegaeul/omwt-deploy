package com.olleh.webtoon.common.dao.toon.domain;

public class TopToonDomain {
	
	private String topfg;	   //상단구분 (day:요일별웹툰 상단, genre:장르별웹툰 상단, webtoon:작품별 상단)
	private String startdt;    //예약전시시작일시 (YYYYMMDDHHMMSS)
	private String enddt;      //예약전시종료일시 (YYYYMMDDHHMMSS)
	private int webtoonseq1;   //첫번째 영역에 들어갈 웹툰순번
	private int webtoonseq2;   //두번째 영역에 들어갈 웹툰순번
	private int webtoonseq3;   //세번째 영역에 들어갈 웹툰순번
	private String thumbpath1; //첫번째 영역 썸네일경로
	private String thumbpath2; //두번째 영역 썸네일경로
	private String thumbpath3; //세번째 영역 썸네일경로
	
	
	public String getTopfg() {
		return topfg;
	}
	public void setTopfg(String topfg) {
		this.topfg = topfg;
	}
	public String getStartdt() {
		return startdt;
	}
	public void setStartdt(String startdt) {
		this.startdt = startdt;
	}
	public String getEnddt() {
		return enddt;
	}
	public void setEnddt(String enddt) {
		this.enddt = enddt;
	}
	public int getWebtoonseq1() {
		return webtoonseq1;
	}
	public void setWebtoonseq1(int webtoonseq1) {
		this.webtoonseq1 = webtoonseq1;
	}
	public int getWebtoonseq2() {
		return webtoonseq2;
	}
	public void setWebtoonseq2(int webtoonseq2) {
		this.webtoonseq2 = webtoonseq2;
	}
	public int getWebtoonseq3() {
		return webtoonseq3;
	}
	public void setWebtoonseq3(int webtoonseq3) {
		this.webtoonseq3 = webtoonseq3;
	}
	public String getThumbpath1() {
		return thumbpath1;
	}
	public void setThumbpath1(String thumbpath1) {
		this.thumbpath1 = thumbpath1;
	}
	public String getThumbpath2() {
		return thumbpath2;
	}
	public void setThumbpath2(String thumbpath2) {
		this.thumbpath2 = thumbpath2;
	}
	public String getThumbpath3() {
		return thumbpath3;
	}
	public void setThumbpath3(String thumbpath3) {
		this.thumbpath3 = thumbpath3;
	}
}