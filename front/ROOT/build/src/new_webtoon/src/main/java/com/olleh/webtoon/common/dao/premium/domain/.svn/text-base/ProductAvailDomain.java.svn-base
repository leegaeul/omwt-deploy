package com.olleh.webtoon.common.dao.premium.domain;

// 이용가능상품 (ow_prd_avail)
public class ProductAvailDomain {

	private int buyseq;			// 구매순번
	private int prdseq;         // 상품순번
	private int prdhistoryseq;	// 상품이력순번
	private String prdfg;       // 상품구분
	private String buyid;		// 구매자ID
	private String idfg;		// 구매자ID구분 (open:오픈ID, olleh:올레ID)
	private String availdt;		// 이용가능일시 (YYYYMMDDHHMISS)
	private String regid;		// 등록자ID
	private String regdt;		// 등록일시 (YYYYMMDDHHMISS)
	
	private int iconseq;        // 아이콘순번  
	private int webtoonseq;		// 작품순번
	private int timesseq;		// 회차순번
	
	public int getBuyseq() {
		return buyseq;
	}
	public void setBuyseq(int buyseq) {
		this.buyseq = buyseq;
	}
	public int getPrdhistoryseq() {
		return prdhistoryseq;
	}
	public void setPrdhistoryseq(int prdhistoryseq) {
		this.prdhistoryseq = prdhistoryseq;
	}
	public String getBuyid() {
		return buyid;
	}
	public void setBuyid(String buyid) {
		this.buyid = buyid;
	}
	public String getIdfg() {
		return idfg;
	}
	public void setIdfg(String idfg) {
		this.idfg = idfg;
	}
	public String getAvaildt() {
		return availdt;
	}
	public void setAvaildt(String availdt) {
		this.availdt = availdt;
	}
	public String getRegid() {
		return regid;
	}
	public void setRegid(String regid) {
		this.regid = regid;
	}
	public String getRegdt() {
		return regdt;
	}
	public void setRegdt(String regdt) {
		this.regdt = regdt;
	}
	public int getWebtoonseq() {
		return webtoonseq;
	}
	public void setWebtoonseq(int webtoonseq) {
		this.webtoonseq = webtoonseq;
	}
	public int getTimesseq() {
		return timesseq;
	}
	public void setTimesseq(int timesseq) {
		this.timesseq = timesseq;
	}
	public int getIconseq() {
		return iconseq;
	}
	public void setIconseq(int iconseq) {
		this.iconseq = iconseq;
	}
	public String getPrdfg() {
		return prdfg;
	}
	public void setPrdfg(String prdfg) {
		this.prdfg = prdfg;
	}
	public int getPrdseq() {
		return prdseq;
	}
	public void setPrdseq(int prdseq) {
		this.prdseq = prdseq;
	}
	
}