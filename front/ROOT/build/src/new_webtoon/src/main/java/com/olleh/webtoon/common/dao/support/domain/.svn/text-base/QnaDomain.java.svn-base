package com.olleh.webtoon.common.dao.support.domain;

public class QnaDomain {
	private int qnaseq; //순번
	private String qnafg; //문의구분
	private String title;       //제목
	private String contents;    //문의내용
	private String email; //문의자 이메일 
	private String phone1; //문의자 전화번호1
	private String phone2; //문의자 전화번호2
	private String statusfg; // 처리상태
	private String reply;// 운영자 답변
	private String replyid;//답변한 운영자ID
	private String replydt; //답변일시
	private String regdt; //등록일자
	private int startRowNo;  	   //해당페이지 시작 Row 번호(페이지크기가 10일 경우 1페이지는 0, 2페이지는 10, 3페이지는 20)
	private int pageSize;    	   //페이지크기	
	private String idfg;		//ID구분 (open:오픈ID, olleh:올레ID)
	private String regid;		//등록자ID
	private String secondreply;  //운영자 2차 답변
	private String secondreplydt; //운영자 2차 답변 등록 일시
	private String ip;//ip주소
	private String servicefg;//서비스구분 (pc : PC, mobile : Mobile, android : Android App, ios : IOS App)

	public String getSecondreply()
	{
		return secondreply;
	}
	public void setSecondreply( String secondreply )
	{
		this.secondreply = secondreply;
	}
	public String getSecondreplydt()
	{
		return secondreplydt;
	}
	public void setSecondreplydt( String secondreplydt )
	{
		this.secondreplydt = secondreplydt;
	}
	public int getQnaseq() {
		return qnaseq;
	}
	public void setQnaseq(int qnaseq) {
		this.qnaseq = qnaseq;
	}
	public String getStatusfg()
	{
		return statusfg;
	}
	public void setStatusfg( String statusfg )
	{
		this.statusfg = statusfg;
	}
	public String getReply()
	{
		return reply;
	}
	public void setReply( String reply )
	{
		this.reply = reply;
	}
	public String getReplyid()
	{
		return replyid;
	}
	public void setReplyid( String replyid )
	{
		this.replyid = replyid;
	}
	public String getReplydt()
	{
		return replydt;
	}
	public void setReplydt( String replydt )
	{
		this.replydt = replydt;
	}
	public String getQnafg() {
		return qnafg;
	}
	public void setQnafg(String qnafg) {
		this.qnafg = qnafg;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContents() {
		return contents;
	}
	public void setContents(String contents) {
		this.contents = contents;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getPhone1() {
		return phone1;
	}
	public void setPhone1(String phone1) {
		this.phone1 = phone1;
	}
	public String getPhone2() {
		return phone2;
	}
	public void setPhone2(String phone2) {
		this.phone2 = phone2;
	}
	public String getRegdt() {
		return regdt;
	}
	public void setRegdt(String regdt) {
		this.regdt = regdt;
	}
	public int getStartRowNo() {
		return startRowNo;
	}
	public void setStartRowNo(int startRowNo) {
		this.startRowNo = startRowNo;
	}
	public int getPageSize() {
		return pageSize;
	}
	public void setPageSize(int pageSize) {
		this.pageSize = pageSize;
	}
	public String getIdfg() {
		return idfg;
	}
	public void setIdfg(String idfg) {
		this.idfg = idfg;
	}
	public String getRegid() {
		return regid;
	}
	public void setRegid(String regid) {
		this.regid = regid;
	}
	public String getIp() {
		return ip;
	}
	public void setIp(String ip) {
		this.ip = ip;
	}
	public String getServicefg() {
		return servicefg;
	}
	public void setServicefg(String servicefg) {
		this.servicefg = servicefg;
	}
}
