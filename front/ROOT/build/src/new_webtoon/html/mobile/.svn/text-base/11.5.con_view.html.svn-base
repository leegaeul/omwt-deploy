	$(window).scroll(function(){
		if (scroll_cnt == 0)
		{
			$('#scroll_conbox_area').append('<div id="scroll_conbox_copy">' + $('#scroll_conbox').html() + '</div>');
			scroll_cnt = 1;
		}
		if ($(this).scrollTop() >= $('#scroll_conbox').offset().top)
		{
			$('#scroll_conbox').css({ visibility: 'hidden' });
			$('#scroll_conbox_copy').show();
		} else {
			$('#scroll_conbox').css({ visibility: 'visible' });
			$('#scroll_conbox_copy').hide();
