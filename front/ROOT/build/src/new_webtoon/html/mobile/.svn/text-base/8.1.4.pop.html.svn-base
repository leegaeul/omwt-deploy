<script type="text/javascript" >
$(document).ready(function(){

	$('.cause input').ezMark();//radio 이미지처리

	//********* 메뉴 레이어 제어관련*********//	
	var lmenuDown = false;
	var lmenuTime = 5000; //레이어메뉴 유지 시간
	var lmenuDownTimer = setTimeout(lmenu_hide,lmenuTime);

	$("header").css("position","fixed");	//상단 헤더 처리

	$('.toonArea').click(function(){//화면 클릭시 레이어 메뉴
		if(lmenuDown==false){
			lmenu_show();			
			clearTimeout(lmenuDownTimer);
			lmenuDownTimer = setTimeout(lmenu_hide,lmenuTime);
		}else{
			lmenu_hide();
		}
	});
	$(window).scroll(function(){//스크롤시 레이어 메뉴
		var posF = $(document).scrollTop();
		if( posF == 0){
			//$('header').fadeIn("fast");
			$('header').css("display","block");
		}else{
			lmenu_hide();
		}
	});
	function lmenu_show(){//레이어 메뉴 show
		$('.toon_tool, header').fadeIn("fast");
		lmenuDown = true;
	}
	function lmenu_hide(){//레이어 메뉴 hide
		$('.toon_tool, header').fadeOut("fast");
		lmenuDown = false;
	}

	$(".btn_top").click(function(e){//상단으로 이동
		$("html,body").stop().animate({'scrollTop':0},500);
		e.preventDefault();
	});	/*end*/		

	$(".btn_reply").click(function(e){//댓글로 이동
		var posY = $(".replyArea").position();		
		$("html,body").stop().animate({'scrollTop':posY.top - 0},500);
		e.preventDefault();
	});	/*end*/		

	/*이모티콘 박스 토글	 20140429 기능 삭제
	$('#imoti_open').toggle(
		function(){
			$("#imotiArea").slideDown("fast");
			$(this).find("img.status").attr("src", $("img.status").attr("src").replace("icn_open@2x.png","icn_close@2x.png"));
		},
		function(){
			$("#imotiArea").slideUp("fast");
			$(this).find("img.status").attr("src", $("img.status").attr("src").replace("icn_close@2x.png","icn_open@2x.png"));
		}
	);*/
	
	function pop_vertical(){//팝업 센터 정렬
		var pop_height = $('.layer_popup .popup').height();
		var pop_height_device = - (pop_height / 2); 
		$('.layer_popup .popup').css("top","50%");
		$('.layer_popup .popup').css("margin-top",pop_height_device);		
	}
	pop_vertical();

});
</script>
